{
    "config": {
        "wifi-ssid": {
            "help": "WiFi SSID",
            "value": "\"WIFI_NAME\""
        },
        "wifi-password": {
            "help": "WiFi Password",
            "value": "\"WIFI_PASSWORD\""
        },
        "esp-rx-pin": {
            "help": "ESP8266 module RX pin",
            "value": "PB_7"
        },
        "esp-tx-pin": {
            "help": "ESP8266 module TX pin",
            "value": "PB_6"
        },
        "mqtt-id": {
            "help": "MQTT Client id",
            "value": "\"JNSK_MQTT_TESTING\""
        },
        "mqtt-broker-ip": {
            "help": "MQTT Broker host ip.",
            "value": "\"xx.xx.xx.xx\""
        },
      "mqtt-broker-hostname": {
            "help": "MQTT Broker resolvable hostname.",
            "value": "\"test.mosquitto.org\""
        },
        "mqtt-broker-port": {
            "help": "MQTT Broker port",
            "value": "1883"
        },
        "mqtt-topic": {
            "help": "MQTT topic to publish",
            "value": "\"JNSK/projekti/testing\""
        }
    },
    "target_overrides": {
        "*": {
            "platform.stdio-convert-newlines": true,
            "esp8266.provide-default" : false,
            "esp8266.built-in-dns": true,
            "target.printf_lib": "std"
        }
    }
}
